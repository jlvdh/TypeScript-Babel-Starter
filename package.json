{
  "name": "babel-typescript-sample",
  "version": "0.7.2",
  "license": "MIT",
  "scripts": {
    "start:dev": "concurrently \"npm:build:watch\" \"npm:pm2:logs\" \"npm:run:dev\"",
    "run:dev": "pm2-runtime start ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:watch": "babel src --out-dir lib --extensions \".ts,.tsx,.js\" --source-maps inline --watch",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx,.js\" --source-maps inline",
    "test": "jest",
    "migrate-seed": "knex migrate:latest --knexfile db/knexfile.js && knex seed:run --knexfile db/knexfile.js",
    "migrate": "docker-compose run --rm api npx knex migrate:latest",
    "rollback": "docker-compose run --rm api npx knex migrate:rollback",
    "seed": "docker-compose run --rm api npx knex seed:run"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.2.3",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/node-fetch": "^2.5.4",
    "babel-jest": "^25.1.0",
    "cors": "^2.8.5",
    "eslint-plugin-jest": "^23.7.0",
    "express": "^4.17.1",
    "knex": "^0.20.9",
    "node-fetch": "^2.6.0",
    "objection": "^2.1.2",
    "pg": "^7.18.1"
  }
}
